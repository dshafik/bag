{
    "name": "dshafik/bag",
    "description": "A comprehensive immutable value objects implementation",
    "type": "library",
    "require": {
        "php": "^8.3",
        "illuminate/collections": "^11.3",
        "illuminate/validation": "^11.3",
        "illuminate/support": "^11.3",
        "laravel/pint": "^1.15"
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "fakerphp/faker": "^1.23",
        "symfony/var-dumper": "*",
        "orchestra/testbench": "^9.0",
        "ramsey/conventional-commits": "dev-allow-sf-7",
        "captainhook/captainhook-phar": "^5.23",
        "larastan/larastan": "^2.0",
        "captainhook/hook-installer": "^1.0"
    },
    "suggest": {
        "illuminate/database": "For sequence support in Bag factories",
        "brick/money": "For Bag objects containing Money, used by the Money cast",
        "prinsfrank/standards": "For Bag objects containing Money/Currency, used by the Money cast"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/llupa/conventional-commits.git"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bag\\": "src/Bag"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Unit\\": "tests/Unit",
            "Tests\\Feature\\": "tests/Featur{{e",
            "Tests\\Fixtures\\": "tests/Fixtures"
        }

    },
    "extra" : {
        "laravel" : {
            "providers" : [
                "BagServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Davey Shafik",
            "email": "davey@php.net"
        }
    ],
    "config": {
        "allow-plugins": {
            "captainhook/captainhook-phar": true,
            "captainhook/hook-installer": true
        }
    }
}
